          SQLTask:SqlStatementSource="merge [stage_db].[dbo].[fact] as t&#xA;using [stage].[dbo].stage as s&#xA;on t.[SalesOrderDetailID] = s.[SalesOrderDetailID]&#xA;when NOT MATCHED BY TARGET  then&#xA;insert &#xA;([SalesOrderID]&#xA;      ,[CustomerID]&#xA;      ,[SalesPersonID]&#xA;      ,[SalesOrderDetailID]&#xA;      ,[ProductID]&#xA;      ,[OrderQty]&#xA;      ,[UnitPrice]&#xA;      ,[UnitPriceDiscount]&#xA;      ,[TotalDue]&#xA;      ,[TaxAmt]&#xA;      ,[ShipDate]&#xA;      ,[DueDate]&#xA;      ,[OrderDate]&#xA;      ,[hash_st]&#xA;)&#xA;values (  s.[SalesOrderID]&#xA;      , s.[CustomerID]&#xA;      , s.[SalesPersonID]&#xA;      , s.[SalesOrderDetailID]&#xA;      , s.[ProductID]&#xA;      , s.[OrderQty]&#xA;      , s.[UnitPrice]&#xA;      , s.[UnitPriceDiscount]&#xA;      , s.[TotalDue]&#xA;      , s.[TaxAmt]&#xA;      , s.[ShipDate]&#xA;      , s.[DueDate]&#xA;      , s.[OrderDate]&#xA;      , s.[hash] ) &#xA;&#x9;  when  MATCHED and t.[hash_st]&lt;&gt; s.[hash] then&#xA;&#x9;  update set [SalesOrderID]=s.[SalesOrderID]&#xA;      ,[CustomerID]= s.[CustomerID]&#xA;      ,[SalesPersonID]= s.[SalesPersonID]&#xA;      ,[SalesOrderDetailID]= s.[SalesOrderDetailID]&#xA;      ,[ProductID]= s.[ProductID]&#xA;      ,[OrderQty]= s.[OrderQty]&#xA;      ,[UnitPrice]= s.[UnitPrice]&#xA;      ,[UnitPriceDiscount]= s.[UnitPriceDiscount]&#xA;      ,[TotalDue]= s.[TotalDue]&#xA;      ,[TaxAmt]= s.[TaxAmt]&#xA;      ,[ShipDate]= s.[ShipDate]&#xA;      ,[DueDate]= s.[DueDate]&#xA;      ,[OrderDate]= s.[OrderDate]&#xA;      ,[hash_st]= s.[hash]&#xA;&#x9;  when NOT MATCHED BY source &#xA;&#x9;  and t.[OrderDate]&gt;= DATEADD(day,?,getdate())  &#xA;&#x9;  then  &#xA; delete&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="$Package::day_n"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="3"
            SQLTask:ParameterSize="-1" />
